dat <- read_sheet('https://docs.google.com/spreadsheets/d/16I5yak3-j3rEqLLcdFCcqzeQSyYjSGihXIbmZxqEILs/edit#gid=2036014622')
### Study data
dat_study <- dat[, c("Study", "Pathogen", "Suceptible_profile", "Resistance_profile", "Country")]
dat_study$Pathogen <- factor(dat_study$Pathogen,
levels = c("S. Typhi",
"S. Typhi and S. Paratyphi",
"S. non-Typhi"))
# Hospitalization day data -- resistance
dat_los_res <- dat[, 53:61]
dat_stu_hos_res <- cbind(dat_study, dat_los_res)
dat_stu_hos_res <- dat_stu_hos_res[(dat_stu_hos_res$Resistance_profile != "Mixed") &
(dat_stu_hos_res$Resistance_profile != "Susceptible") &
(dat_stu_hos_res$`How many days did the hospitalisation last?_resistant` != "NULL"), ]
# Hospitalization day data -- susceptible
dat_los_sus <- dat[, 44:52]
dat_stu_hos_sus <- cbind(dat_study, dat_los_sus)
dat_stu_hos_sus <- dat_stu_hos_sus[(dat_stu_hos_sus$Suceptible_profile != "Mixed") &
!is.na(dat_stu_hos_sus$Suceptible_profile) &
(dat_stu_hos_sus$`How many days did the hospitalisation last?_susceptible` != "NULL"), ]
# create function to generate forest plot
forest_plot <- function(dat_stu_hos, arrange_by, file_name){
dat_stu_hos <- data.table(dat_stu_hos)
colnames(dat_stu_hos)[6:14] <- c("day", "statistical_measure", "comment", "uncertainty/range",
"uncertainty/range_comment", "higher", "lower", "other", "n")
dat_stu_hos$day <- as.numeric(dat_stu_hos$day)
dat_stu_hos[dat_stu_hos == "NULL"] <- NA
dat_stu_hos$higher <- as.numeric(dat_stu_hos$higher)
dat_stu_hos$lower <- as.numeric(dat_stu_hos$lower)
# adjust variables
dat_stu_hos[statistical_measure == "Mean", Mean := day]
dat_stu_hos[statistical_measure == "Median", Median := day]
dat_stu_hos[`uncertainty/range` == "Interquartile range", higher_i := higher]
dat_stu_hos[`uncertainty/range` == "Interquartile range", lower_i := lower]
dat_stu_hos[`uncertainty/range` == "Range", higher_r := higher]
dat_stu_hos[`uncertainty/range` == "Range", lower_r := lower]
dat_stu_hos[`uncertainty/range` == "Standard deviation", SD := other]
# dat_meta <- dat_stu_hos[!(is.na(higher) & statistical_measure == "Median"), ]
# dat_meta <- dat_meta[!is.na(`uncertainty/range`), ]
# dat_meta <- dat_meta[dat_meta$Pathogen != "Salmonella non-typhi", ]
dat_meta <- dat_stu_hos %>% arrange(Pathogen, paste(arrange_by))
# meta & forestplot -- Salmonella_typhi
ma.data <- metamean(n = n,
mean = Mean,
data = dat_meta,
sd = SD,
median = Median,
q1 = lower_i,
q3 = higher_i,
min = lower_r,
max = higher_r,
#                   subgroup = Pathogen,
common = FALSE,
random = TRUE,
overall = FALSE,
overall.hetstat = FALSE)
png(file = paste(file_name), width = 3300, height = 1100, res = 300)
forest(ma.data,
leftcols = c("Study", "Pathogen", paste(arrange_by), "Country"),
leftlabs = c("Study", "Pathogen", "Resistance", "Country"),
rightlabs = c("Mean", "95% CI"),
#      subgroup = FALSE,
xlim = c(1,15),
print.byvar = TRUE)
dev.off()
return(ma.data)}
